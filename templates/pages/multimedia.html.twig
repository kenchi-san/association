{% extends 'base.html.twig' %}
{% block title %}
    <title>vidéo de l'association Swahilisa</title>
    <meta name="description" content="La vie de swahilisa en live. Notre passion et notre aventure en vidéo" />
{% endblock %}
{% block body %}
    <div class="container">


        <div class="row">


            {% for video in videos %}
                <div class="col-sm-3 nopadding">
                    <div class="embed-responsive embed-responsive-16by9">
                        {{ video.link |raw }}
                    </div>
                </div>
            {% endfor %}

        </div>

    </div>
{% endblock %}